<Sysmon schemaversion="4.30">
  <EventFiltering>
    <RuleGroup name="" groupRelation="or">
      <FileCreateTime onmatch="exclude">
        <Image condition="image">OneDrive.exe</Image>        <!--OneDrive constantly changes file times-->
      </FileCreateTime>
    </RuleGroup>
  </EventFiltering>
</Sysmon>